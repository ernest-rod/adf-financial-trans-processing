{
	"name": "pl_load_trans_into_staging_area",
	"properties": {
		"activities": [
			{
				"name": "Load Data Into Staging Table",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFileName": "*.parquet",
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "TRUNCATE TABLE bank.stage_transactions",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Prop_0",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "trans_date",
									"type": "DateTime",
									"physicalType": "date"
								}
							},
							{
								"source": {
									"name": "Prop_1",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "trans_desc",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "Prop_2",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "trans_original_desc",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "Prop_3",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "trans_amount",
									"type": "Decimal",
									"physicalType": "smallmoney",
									"scale": 4,
									"precision": 10
								}
							},
							{
								"source": {
									"name": "Prop_4",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "trans_type",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "Prop_5",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "trans_category",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "Prop_6",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "trans_account_name",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "Prop_7",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "trans_label",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "Prop_8",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "trans_notes",
									"type": "String",
									"physicalType": "varchar"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_transactions_in_delta_stage",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_poseidon_bank_stage_table",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Database"
		},
		"annotations": []
	}
}