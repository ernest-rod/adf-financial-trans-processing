{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-financial-trans-processing"
		},
		"lnk_on_premise_data_export_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'lnk_on_premise_data_export'"
		},
		"lnk_poseidon_database_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lnk_poseidon_database'"
		},
		"lnk_abls_axiomstorage4inbounddata_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://axiomstorage4inbounddata.blob.core.windows.net"
		},
		"lnk_adls_axiomstandardatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://axiomstandarddatalake.dfs.core.windows.net"
		},
		"lnk_on_premise_data_export_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\mykonos\\C$\\Users\\aspac\\data\\exports"
		},
		"lnk_on_premise_data_export_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "aspac"
		},
		"trg_sevnt_axiomstorage4inbounddata_inbounddatafiles_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/b422d504-452d-4c32-86ff-df16b9c82ff1/resourceGroups/AxiomCore_StorageAccounts/providers/Microsoft.Storage/storageAccounts/axiomstorage4inbounddata"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-financial-trans-processing_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lnk_poseidon_database_connectionString": {
						"value": "[parameters('lnk_poseidon_database_connectionString')]"
					},
					"lnk_on_premise_data_export_password": {
						"value": "[parameters('lnk_on_premise_data_export_password')]"
					},
					"lnk_abls_axiomstorage4inbounddata_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('lnk_abls_axiomstorage4inbounddata_properties_typeProperties_serviceEndpoint')]"
					},
					"lnk_adls_axiomstandardatalake_properties_typeProperties_url": {
						"value": "[parameters('lnk_adls_axiomstandardatalake_properties_typeProperties_url')]"
					},
					"lnk_on_premise_data_export_properties_typeProperties_host": {
						"value": "[parameters('lnk_on_premise_data_export_properties_typeProperties_host')]"
					},
					"lnk_on_premise_data_export_properties_typeProperties_userId": {
						"value": "[parameters('lnk_on_premise_data_export_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-financial-trans-processing_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"trg_sevnt_axiomstorage4inbounddata_inbounddatafiles_properties_typeProperties_scope": {
						"value": "[parameters('trg_sevnt_axiomstorage4inbounddata_inbounddatafiles_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-financial-trans-processing_ArmTemplate_0')]"
			]
		}
	]
}